.agents{
    min-height: calc(100vh - 377px);
    background: $base-background-color ;
    color:$base-font-color;
    .agent-conten{
          width: 1200px;
            margin: 0 auto;
            margin-top: 30px;
            border-radius: 2px;
            background:$sub-background-color ;
            // margin-bottom:20px;
            // min-height:400px;
            .title{
                height:60px;
                line-height: 60px;
                padding-left: 30px;
                font-size: 18px;
                background: $public-header-background;
            }
            .invitations{
                border-bottom: 30px solid $base-background-color;
                .content{
                    text-align: center;
                    margin-top: 60px;
                    margin-bottom: 73px;
                    .con-title{
                        color: $gray-font-color;
                        .effective{
                            display: inline-block;
                            margin-right: 118px;
                        }
                    }
                    .con-data{
                        margin-top:29px;
                          .total-number,.oblique,.effec-number{
                              display: inline-block;
                            margin-right:60px;
                            font-size: 20px;
                        }
                             .total-number{
                                 margin-right: 0;
                             }
                      
                    }
                    
                }
            }
            .maid{
                margin-bottom: 20px;
                padding-bottom: 48px;
                .cale{
                     width:954px;
                     margin: 0 auto;
                     margin-top: 33px;
                     margin-bottom: 48px;
                     .income-con{
                         float:left;
                     }
                     .button{
                         float: right ;
                         button{
                             width: 80px;
                             height:28px;
                             a{
                                 color: $white-color;
                             }
                         }
                     }
                } 
                .calendar{
                    margin-top: 25px;
                    border:5px solid  $public-header-background;
                    .title{
                        cursor:pointer;
                        text-align: center;
                        .last-year,.last-month{
                            margin-right: 26px;
                        }
                        .next-month,.next-year{
                            margin-left: 26px;
                        }
                    }
                    .date{
                        .week{
                            height: 36px;
                            line-height: 36px;
                            b{
                                display: inline-block;
                                width: 13.9%;
                                text-align: center;
 
                            }
                        }
                        .day{
                            display:block;
                           width: 14.07%;
                           float:left;
                            height:57px;
                            text-align: center;
                            border: 1px solid $public-header-background;
                            
                           
                            .date{
                                display: block;
                                margin: 11px 0;
                                cursor:pointer;
                            }
                            .data{
                                cursor:pointer;
                            }
                        }
                        .deactive{
                      
                             background: $blue-font-color;
                        }
                        .days{
                            opacity:0
                        }
                    }
                }  
               
            }
    }
    .projectile{
        position: fixed;
        height: 100%;
        height:100%;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        z-index:10;
        background: rgba(0,0,0,.6);
        .expor-list{
            width: 1070px;
            // height:569px;
            background: $base-background-color;;
            position: absolute;
            top: 50%;
            left: 50%;
            margin-top: -285px;
            margin-left: -535px;
            border: 7px solid $sub-background-color 
              table{
                thead{
                    .title{
                        position:relative;
                     
                    }
                       span{
                            position: absolute;
                            top: 15px;
                            right:20px;
                            cursor:pointer;
                            color: $blue-font-color;
                        }
                     th{
                        width:1%;
                        height:45px;
                        line-height:45px;
                        color:$explain-font-color;
                        background: $sub-background-color 
                    }
                   
                }
                tbody{
                    text-align:center;
                    tr{
                         height:45px;
                        line-height:45px;
                    }
                    .interlacing{
                        background: $sub-background-color 
                    }
                }
            }
            .no_data{
                line-height: 200px;
                text-align: center;
            }
        }
    }
}